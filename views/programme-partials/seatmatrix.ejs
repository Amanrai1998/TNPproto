    <section>
        <h2>Seat Matrix</h2>
        <h2 class="mini-heading"><%= seats[0].courseName %></h2>
        <table>
            <thead class="table-head">
                <tr>
                    <th><p>S.No</p></th>
                    <th><p>Department Name</p></th>
                    <th><p>No. of Students Registered</p></th>
                </tr>
            </thead>
            <tbody class="red-green mod1 table-body">
                <% seats[0].data.forEach((tabledata,i)=>{ %>
                    <tr>
                        <td><p><%= i+1 %></p></td>
                        <td><p><%= tabledata.departmentName %></p></td>
                        <td><p><%= tabledata.seat %></p></td>
                    </tr>
                <% }) %>
            </tbody>
            <tfoot class="table-foot">
                <tr>
                    <th colspan="2"><p>Total No. of Students Registered</p></th>
                    <th><p><%= seats[0].totalSeats %></p></th>
                </tr>
            </tfoot>
        </table>
        <h2 class="mini-heading"><%= seats[1].courseName %></h2>
        <table>
            <thead class="table-head">
                <tr>
                    <th><p>S.No</p></th>
                    <th><p>Department Name</p></th>
                    <th><p>Program Name</p></th>
                    <th><p>No. of Students Registered</p></th>
                </tr>
            </thead>
            <tbody class="red-green mod1 table-body">
                <% seats[1].data.forEach((tabledata,i)=>{ %>
                    <tr>
                        <td><p><%= i+1 %></p></td>
                        <td><p><%= tabledata.departmentName %></p></td>
                        <td><% tabledata.program.forEach(program=>{ %>
                                <p><%= program.name %></p>
                            <% }) %></td>
                        <td><p><%= tabledata.seat %></p></td>
                    </tr>
                <% }) %>
            </tbody>
            <tfoot class="table-foot">
                <tr>
                    <th colspan="3"><p>Total No. of Students Registered</p></th>
                    <th><p><%= seats[1].totalSeats %></p></th>
                </tr>
            </tfoot>
        </table>
        <h2 class="mini-heading"><%= seats[2].courseName %></h2>
        <table>
            <thead class="table-head">
                <tr>
                    <th><p>S.No</p></th>
                    <th><p>Specialization</p></th>
                    <th><p>Courses</p></th>
                    <th><p>Intake</p></th>
                </tr>
            </thead>
            <tbody class="red-green mod1 table-body">
                <tr>
                    <td><p>1</p></td>
                    <td><p>Technical Specialization</p></td>
                    <% seats[2].data.forEach(tabledata=>{ %>
                        <td><% tabledata.technicalSpec.forEach(course=>{ %>
                            <p><%= course.name %></p>
                        <% }) %></td>
                    <% }) %>
                    <td><p> </p></td>
                </tr>
                <tr>
                    <td><p>2</p></td>
                    <td><p>Functional Specialization</p></td>
                    <% seats[2].data.forEach(tabledata=>{ %>
                        <td><% tabledata.functionalSpec.forEach(course=>{ %>
                            <p><%= course.name %></p>
                        <% }) %></td>
                        <td><p><%= tabledata.seat %></p></td>
                    <% }) %>
                </tr>
            </tbody>
            <tfoot class="table-foot">
                <tr>
                    <th colspan="3"><p>Total(Masters of Administration Program)</p></th>
                    <th><p><%= seats[2].totalSeats %></p></th>
                </tr>
            </tfoot>
        </table>
    </section>